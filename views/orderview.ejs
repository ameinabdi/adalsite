<%- include('header') %>

<div class="page-wrapper">
        <!-- ============================================================== -->
        <!-- Bread crumb and right sidebar toggle -->
        <!-- ============================================================== -->
        <div class="page-breadcrumb">
            <div class="row">
                <div class="col-5 align-self-center">
                    <div class="d-flex align-items-center">

                    </div>
                </div>
                <div class="col-7 align-self-center">
                    <div class="d-flex no-block justify-content-end align-items-center">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item">
                                    <a href="#">Home</a>
                                </li>
                                <li class="breadcrumb-item active" aria-current="page">orderdetail</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>


<div class="container-fluid">
        <!-- ============================================================== -->
        <!-- Start Page Content -->
        <!-- ============================================================== -->
        <!-- Row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-header bg-danger">
                        <h4 class="m-b-0 text-white">Order Detail</h4>
                    </div>
                    <% singleorder.forEach(function (info) { %>
                       
                    <form class="form-horizontal">
                        <div class="form-body">
                            <div class="card-body">
                                <h4 class="card-title">Order Information</h4>
                            </div>
                            <hr class="m-t-0 m-b-40">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="control-label text-right col-md-3">Costumer Name:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static">  <%= info.fullname %> 
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="control-label text-right col-md-3">Telephone:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static"> <%= info.phone %>  </p>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                </div>
                                <!--/row-->
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="control-label text-right col-md-3">Email:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static"> <%= info.email %>  </p>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="control-label text-right col-md-3">Product Name:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static"> <%= info.pro_name %>  </p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--/row-->
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="control-label text-right col-md-3">Price:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static"> $-<%= info.price %>  </p>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="control-label text-right col-md-3">shipping:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static"> <%= info.delivery %>  </p>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                </div>
                                <!--/row-->
                                <h2 class="card-title">Address</h2>
                            </div>
                            <hr class="m-t-0 m-b-40">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="control-label text-right col-md-3">Address 1:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static"><%= info.address1 %> </p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="control-label text-right col-md-3">Address2:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static"> <%= info.address2 %>  </p>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="control-label text-right col-md-3">Town:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static"> <%= info.town %>  </p>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                </div>
                                <!--/row-->
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="control-label text-right col-md-3">Payment:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static"> <%= info.payment %>  </p>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="control-label text-right col-md-3">Date:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static"> <%= info.ordertime %>  </p>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                </div>
                            </div>
                            <hr>
                            <div class="form-actions">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="row">
                                                <div class="col-md-offset-3 col-md-9">
                                                    <button class="btn btn-danger" onclick="printer()"> <i class="fa fa-pencil"></i> Print</button>
                                                    <script>
                                                        function printer(){
                                                            window.print();
                                                        }
                                                    </script>
                                                    <a href="/orderlist" class="btn btn-dark">Cancel</a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6"> </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <% }) %>
                </div>
            </div>
        </div>




<%- include('footer') %>
 
 